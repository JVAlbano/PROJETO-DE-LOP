var vxo= []
var vyo= []
var vtam= []
var qtobjetos = 20
function setup() {
  createCanvas(500, 500);
  for( var i=0;i < qtobjetos;i++){
    vxo[i]= random(0,500);
    vyo[i]= random(0,500);
    vtam[i]= random (2,40)
  }
}

function draw() {
  background(220);
  for(var i=0;i < qtobjetos; i++){
    ellipse(vxo[i],vyo[i], vtam[i],vtam[i])
}
}
